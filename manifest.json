{
    "manifest_version" : 2, 
    "name" : "KidsMode", 
    "description" : "Mental health support and anti-cyberbullying",
    "version" : "1.0.0",
    "icons": { "128": "tab-icon-2.png" },
    "author" : "Hacker Kings",
    "browser_action" : {
        "default_icon" : "tab-icon-2.png",
        "default_popup" : "default_popup.html"
    },
    "permissions" : ["activeTab"],
    "content_scripts" : [ 
      { 
          "js": [ "jquery-3.5.1.min.js", "filter.js" ],
          "matches": [ "http://*/*", "https://*/*"]
      }
    ]
}
